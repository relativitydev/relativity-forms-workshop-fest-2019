{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "impliedStrict": true
        }
    },
	"plugins": [
		"es5"
	],
	"extends": [
		"eslint:recommended",
		"plugin:es5/no-es2015"
	],
	"rules": {
		"no-trailing-spaces": 0,
		"no-fallthrough": 1,
		"no-debugger": 1,
		"no-empty": 1,
        "no-lone-blocks": 0,
		"no-console": 0,
		"curly": 1,
		"default-case": 0,
		"no-alert": 0,
		"no-empty-function": 1,
		"no-mixed-spaces-and-tabs": 0,
        "semi": 2
	}
}